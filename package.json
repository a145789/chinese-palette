    "gh-pages": "^6.0.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-copy": "^1.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "vite": "^4.4.11",
    "vite-plugin-wasm-pack": "^0.1.12"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@ikun-ui/backtop": "0.0.9",
    "@ikun-ui/icon": "0.0.9",
    "@ikun-ui/input": "0.0.9",
    "@ikun-ui/message": "0.0.9",
